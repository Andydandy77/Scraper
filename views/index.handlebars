    <div class = "banner">
        <h1>Find A Tweet</h1>
    </div>
<div class="container">
    
    
    <form>
    <div class="form-group">
        <label for="User">Twitter Name</label>
        <input type="name" class="form-control" id="name"  placeholder="Enter name">
    </div>

    <form>
    <div class="form-group">
        <label for="exampleInputEmail1">Keyword</label>
        <input type="keyword" class="form-control" id="keyword" placeholder="Enter keyword">
    </div>
    </form>
    
    
    <button id ="submit" type="submit" class="btn btn-primary">Submit</button>

    {{#each tweet}}
        <div class = "tweetContainer col-lg-12">
            <img class="profileImg" src={{this.profileImg}}>
            <h3 class="name">{{this.name}}</h3>
            <p class="date">{{this.date}}</p>
            <p class = "tweet"> {{this.tweet}}</p>
            {{#if this.photoLink}}
            <img class="photo" style="height:50%; width: 50%; position: relative; top: 50px; left: 125px; margin-bottom: 75px " src = {{this.photoLink}}>
            {{/if}}


        </div>
    {{/each}}
    
        
    

</div>